<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoSuite">

	<test name="getRowCount">
		<classes>
			<class name="LoginExcelTestNG.ExcelLoginTestNG">
				<methods>
					<include name="getRowCount" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="getCellData">
		<classes>
			<class name="LoginExcelTestNG.ExcelLoginTestNG">
				<methods>
					<include name="getCellData" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="validLogin">
		<classes>
			<class name="LoginExcelTestNG.ExcelLoginTestNG">
				<methods>
					<include name="setUp" />
					<include name="setUpTest" />
					<include name="validLogin" />
					<include name="verifyLogin" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="inValidLogin">
		<classes>
			<class name="LoginExcelTestNG.ExcelLoginTestNG">
			<methods>
			<include name="inValidLogin"/>
			</methods>
			</class>
		</classes>
	</test>

</suite> <!-- Suite -->
